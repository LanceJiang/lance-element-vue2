{
  "name": "adber-ui",
  "version": "1.0.5",
  "description": "vue-cli3构建基于elementUI二次封装库 集成 vxe-table",
  "main": "lib/adber-ui.umd.min.js",
  "keywords": [
    "element-ui",
    "vxe-table",
    "adber-ui",
    "vue",
    "components"
  ],
  "files": [
    "lib",
    "src",
    "packages",
    "types",
    "style"
  ],
  "typings": "types/index.d.ts",
  "private": false,
  "author": "LanceJiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LanceJiang/adber-ui/issues"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib:packages": "vue-cli-service build --target lib --name adber-ui --dest lib packages/index.js",
    "lib:packages-report": "vue-cli-service build --target lib --name adber-ui --dest lib packages/index.js --report",
    "lib:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "lib": "npm run lib:packages && npm run lib:utils",
    "lib2publish": "npm run lib && npm publish"
  },
  "dependencies": {
    "decimal.js": "^10.2.1",
    "jsbarcode": "^3.11.5",
    "pdfjs-dist": "2.2.228",
    "vuedraggable": "^2.24.3",
    "vxe-table": "3.2.20",
    "xe-utils": "^3.5.7"
  },
  "peerDependencies": {
    "element-ui": "^2.15.8",
    "vue": "^2.6.6",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vuedraggable": "^2.24.3",
    "vue-i18n": "^8.18.2",
    "vxe-table": "3.2.20",
    "xe-utils": "^3.5.7",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "jsbarcode": "^3.11.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.25.5",
    "cross-env": "^3.1.3",
    "element-ui": "^2.15.8",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "style-resources-loader": "^1.3.3",
    "typescript": "~3.9.3",
    "vue": "^2.6.6",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.21",
    "vuex": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LanceJiang/adber-ui"
  }
}
